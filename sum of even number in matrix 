        "#51\n",
        "a,b=list(map(int,input().split()))\n",
        "l=[input().split() for i in range(a)]\n",
        "c=[]\n",
        "for i in range(len(l)):\n",
        "  for j in range(len(l[i])):\n",
        "    if int(l[i][j])%2==0:\n",
        "      c.append(int(l[i][j]))\n",
        "d=0\n",
        "for i in c:\n",
        "  d=d+int(i)\n",
        "print(d)\n",
        "\n",
        "\n"
